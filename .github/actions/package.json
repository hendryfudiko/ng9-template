{
  "name": "custom-workflows",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript custom workflows",
  "author": "CATAPA Dev",
  "license": "MIT",
  "scripts": {
    "format-check:test-ts-action": "prettier --check ./test-ts-action/**/*.ts",
    "format:test-ts-action": "prettier --write ./test-ts-action/**/*.ts",
    "lint:test-ts-action": "eslint ./test-ts-action/src/**/*.ts",
    "package:test-ts-action": "ncc build ./test-ts-action/src/main.ts --out ./test-ts-action/dist --source-map",
    "all:test-ts-action": "npm run format:test-ts-action && npm run lint:test-ts-action && npm run package:test-ts-action"
  },
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0",
    "@octokit/auth-app": "^2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.7.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}
